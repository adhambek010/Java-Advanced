package com.java.advanced.generics;

public class SingleGenericHolder<T> {
	
	T obj;
	
	public SingleGenericHolder(T obj) {
		this.obj = obj;
	}
	
	public T getObject() {
		return this.obj;
	}
	
	public static void main(String[] args) {
		SingleGenericHolder<Integer> genericHolder = new SingleGenericHolder<Integer>(10);
		System.out.println(genericHolder.getObject());
		
		SingleGenericHolder<String> genericHolder
	}
}
